<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/dashboard.css">
</head>

<body>
    <div class="wrapper">
        <%-include('navbar')%>
            <div class="course-container">
                <h1>Courses</h1>
                <%course.forEach(i=>{%>
                    <div class="course">
                        <h4>
                            <%=i.course%>
                        </h4>
                        <a href="/admin/course/<%= i.course%>">View</a>
                    </div>
                    <%})%>
            </div>
    </div>
</body>

</html>